<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.production.jpsc.student.Documents"
    tools:showIn="@layout/activity_documents"
    android:background="@drawable/cover">


    <ImageButton
        android:layout_width="130.5sp"
        android:layout_height="130sp"
        android:id="@+id/imageButton"
        android:padding="0dp"
        android:layout_gravity="left"
        android:src="@drawable/cs"
        android:scaleType="fitXY"
        android:onClick="cs"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp" />
    <ImageButton
        android:layout_width="130.5sp"
        android:layout_height="130sp"
        android:id="@+id/image"
        android:padding="0dp"
        android:layout_gravity="right"
        android:src="@drawable/mechanical"
        android:scaleType="fitXY"
        android:onClick="me"
        android:layout_above="@+id/ima"
        android:layout_alignLeft="@+id/imageButton3"
        android:layout_alignStart="@+id/imageButton3" />

    <ImageButton
        android:layout_width="130.5sp"
        android:layout_height="130sp"
        android:id="@+id/ima"
        android:padding="0dp"
        android:layout_gravity="right"
        android:src="@drawable/civil"
        android:scaleType="fitXY"
        android:onClick="civil"
        android:layout_marginTop="30dp"
        android:layout_below="@+id/imageButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="130.5sp"
        android:layout_height="130sp"
        android:id="@+id/imageButton3"
        android:padding="0dp"
        android:layout_gravity="right"
        android:src="@drawable/elect"
        android:scaleType="fitXY"
        android:onClick="ele"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/ima" />

    <ImageButton
        android:layout_width="130.5sp"
        android:layout_height="130sp"
        android:id="@+id/imageButton4"
        android:padding="0dp"
        android:layout_gravity="right"
        android:src="@drawable/ec"
        android:scaleType="fitXY"
        android:onClick="ec"
        android:layout_below="@+id/ima"
        android:layout_marginTop="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="130.5sp"
        android:layout_height="130sp"
        android:id="@+id/imageButton2"
        android:padding="0dp"
        android:layout_gravity="right"
        android:src="@drawable/its"
        android:scaleType="fitXY"
        android:onClick="c"
        android:layout_below="@+id/imageButton3"
        android:layout_marginTop="30dp"
        android:layout_alignLeft="@+id/imageButton3"
        android:layout_alignStart="@+id/imageButton3" />


</RelativeLayout>
